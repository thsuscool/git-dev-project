<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="xml" class="com.neighborsystem.durian.restapi.view.XmlFormat">
		<property name="contentType">
			<value>text/xml;charset=UTF-8</value>
		</property>
		<property name="charset" value="UTF-8" />
	</bean>

	<bean id="json" class="com.neighborsystem.durian.restapi.view.JsonFormat">
		<property name="contentType">
			<value>text/json;charset=UTF-8</value>
		</property>
	</bean>
	<bean id="api" class="kostat.sop.ServiceAPI.view.ApiFormat">
		<property name="contentType">
			<value>text/json;charset=UTF-8</value>
		</property>
	</bean>
	<bean id="kml" class="kostat.sop.ServiceAPI.view.KmlFormat">
		<property name="contentType">
			<value>text/xml;charset=UTF-8</value>
		</property>
		<property name="charset" value="UTF-8" />
	</bean>
	
	<bean id="geojson" class="kostat.sop.ServiceAPI.view.GeoJsonFormat">
		<property name="contentType">
			<value>application/json;charset=UTF-8</value>
		</property>
		<property name="charset" value="UTF-8" />
	</bean>
	
	<bean id="form" class="com.neighborsystem.durian.restapi.view.JsonFormat">
        <property name="contentType">
            <value>text/html;charset=UTF-8</value>
        </property>
    </bean>
    
    <bean id="download" class="kostat.sop.ServiceAPI.view.DownloadView">
    </bean>
    
    <bean id="excel" class="kostat.sop.ServiceAPI.view.ExcelFormat">
		<property name="contentType">
			<!-- Excel 2007 이상 .xlsx files -->
			<!--<value>application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</value>-->
			<!-- Excel .xls files -->
			<value>application/vnd.ms-excel</value>
		</property>
	</bean>
    <bean id="jsonp" class="kostat.sop.ServiceAPI.view.JsonPView">
    </bean>
</beans>